// Computes the n-th Fibonacci number.
i32 fibonacci(i32 n) {
    if (n <= 0) { return 0; }
    if (n == 1) { return 1; }

    i32 f1 = 0;
    i32 f2 = 1;

    for (i32 i = 1, i < n, i++) {
        i32 f3 = f1 + f2;
        f1 = f2;
        f2 = f3;
    }

    return f2;
}

i32 main() {
    i32 result = fibonacci(10);

    @print(result);

    return 0;
}
